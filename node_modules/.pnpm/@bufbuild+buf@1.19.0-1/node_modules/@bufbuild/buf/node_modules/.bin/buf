#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@bufbuild+buf@1.19.0-1/node_modules/@bufbuild/buf/bin/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@bufbuild+buf@1.19.0-1/node_modules/@bufbuild/buf/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@bufbuild+buf@1.19.0-1/node_modules/@bufbuild/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@bufbuild+buf@1.19.0-1/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@bufbuild+buf@1.19.0-1/node_modules/@bufbuild/buf/bin/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@bufbuild+buf@1.19.0-1/node_modules/@bufbuild/buf/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@bufbuild+buf@1.19.0-1/node_modules/@bufbuild/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@bufbuild+buf@1.19.0-1/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/buf" "$@"
else
  exec node  "$basedir/../../bin/buf" "$@"
fi
