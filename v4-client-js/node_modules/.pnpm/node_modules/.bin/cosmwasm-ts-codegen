#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/ts-codegen/main/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/ts-codegen/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules:/home/ec2-user/ethos-integration/stream-chain/node_modules:/home/ec2-user/ethos-integration/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@cosmwasm/ts-codegen/main/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@cosmwasm/ts-codegen/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@cosmwasm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/ts-codegen/main/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/ts-codegen/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules:/home/ec2-user/ethos-integration/stream-chain/node_modules:/home/ec2-user/ethos-integration/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@cosmwasm/ts-codegen/main/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@cosmwasm/ts-codegen/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@cosmwasm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@cosmwasm/ts-codegen/main/ts-codegen.js" "$@"
else
  exec node  "$basedir/../@cosmwasm/ts-codegen/main/ts-codegen.js" "$@"
fi
